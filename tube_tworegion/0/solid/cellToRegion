FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/solid";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    wall_out
    {
        type            zeroGradient;
    }
    wall_in
    {
        type            zeroGradient;
    }
    solid_in
    {
        type            zeroGradient;
    }
    solid_out
    {
        type            zeroGradient;
    }
}


